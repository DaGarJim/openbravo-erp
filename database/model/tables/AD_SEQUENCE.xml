<?xml version="1.0"?>
<database name="TABLE AD_SEQUENCE">
    <table name="AD_SEQUENCE" primaryKey="AD_SEQUENCE_KEY">
      <column name="AD_SEQUENCE_ID" primaryKey="true" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="AD_CLIENT_ID" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="AD_ORG_ID" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="ISACTIVE" primaryKey="false" required="false" type="CHAR" size="1" default="Y" autoIncrement="false"/>
      <column name="CREATED" primaryKey="false" required="true" type="TIMESTAMP" size="7" default="SYSDATE" autoIncrement="false"/>
      <column name="CREATEDBY" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="UPDATED" primaryKey="false" required="true" type="TIMESTAMP" size="7" default="SYSDATE" autoIncrement="false"/>
      <column name="UPDATEDBY" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="NAME" primaryKey="false" required="true" type="VARCHAR" size="60" autoIncrement="false"/>
      <column name="DESCRIPTION" primaryKey="false" required="false" type="NVARCHAR" size="255" autoIncrement="false"/>
      <column name="VFORMAT" primaryKey="false" required="false" type="NVARCHAR" size="40" autoIncrement="false"/>
      <column name="ISAUTOSEQUENCE" primaryKey="false" required="true" type="CHAR" size="1" default="Y" autoIncrement="false"/>
      <column name="INCREMENTNO" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="STARTNO" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="CURRENTNEXT" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="CURRENTNEXTSYS" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="ISAUDITED" primaryKey="false" required="false" type="CHAR" size="1" default="N" autoIncrement="false"/>
      <column name="ISTABLEID" primaryKey="false" required="false" type="CHAR" size="1" default="N" autoIncrement="false"/>
      <column name="PREFIX" primaryKey="false" required="false" type="NVARCHAR" size="10" autoIncrement="false"/>
      <column name="SUFFIX" primaryKey="false" required="false" type="NVARCHAR" size="10" autoIncrement="false"/>
      <column name="STARTNEWYEAR" primaryKey="false" required="false" type="CHAR" size="1" default="N" autoIncrement="false"/>
      <check name="ADSEQ_ISACTIVE_CHECK" condition="IsActive in ('Y','N')"/> 
      <check name="ADSEQ_ISAUTOSEQUENCE_CHECK" condition="IsAutoSequence in ('Y','N')"/> 
      <check name="ADSEQ_ISAUDITED_CHECK" condition="IsAudited in ('Y','N')"/> 
      <check name="ADSEQ_ISTABLEID_CHECK" condition="IsTableID in ('Y','N')"/> 
      <check name="ADSEQ_STARTNEWYEAR_CHECK" condition="StartNewYear in ('Y','N')"/> 
      <foreign-key foreignTable="AD_CLIENT" name="SEQUENCECLIENT">
        <reference local="AD_CLIENT_ID" foreign="AD_CLIENT_ID"/>
      </foreign-key>
      <foreign-key foreignTable="AD_ORG" name="SEQUENCEORG">
        <reference local="AD_ORG_ID" foreign="AD_ORG_ID"/>
      </foreign-key>
      <unique name="AD_SEQUENCE_NAME">
        <unique-column name="AD_CLIENT_ID"/>
        <unique-column name="NAME"/>
      </unique>
    </table>
</database>
