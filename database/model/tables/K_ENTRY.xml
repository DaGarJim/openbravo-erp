<?xml version="1.0"?>
  <database name="TABLE K_ENTRY">
    <table name="K_ENTRY" primaryKey="K_ENTRY_KEY">
      <column name="K_ENTRY_ID" primaryKey="true" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="AD_CLIENT_ID" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="AD_ORG_ID" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="ISACTIVE" primaryKey="false" required="true" type="CHAR" size="1" default="Y" autoIncrement="false"/>
      <column name="CREATED" primaryKey="false" required="true" type="TIMESTAMP" size="7" default="SYSDATE" autoIncrement="false"/>
      <column name="CREATEDBY" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="UPDATED" primaryKey="false" required="true" type="TIMESTAMP" size="7" default="SYSDATE" autoIncrement="false"/>
      <column name="UPDATEDBY" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="NAME" primaryKey="false" required="true" type="NVARCHAR" size="60" autoIncrement="false"/>
      <column name="K_TOPIC_ID" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="TEXT" primaryKey="false" required="true" type="NVARCHAR" size="2000" autoIncrement="false"/>
      <column name="RATING" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="ISPUBLIC" primaryKey="false" required="true" type="CHAR" size="1" default="Y" autoIncrement="false"/>
      <column name="KEYWORDS" primaryKey="false" required="false" type="NVARCHAR" size="255" autoIncrement="false"/>
      <column name="K_SOURCE_ID" primaryKey="false" required="false" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="DESCRIPTIONURL" primaryKey="false" required="false" type="NVARCHAR" size="120" autoIncrement="false"/>
      <column name="VALIDTO" primaryKey="false" required="false" type="TIMESTAMP" size="7" autoIncrement="false"/>
      <foreign-key foreignTable="AD_CLIENT" name="ADCLIENT_KENTRY">
        <reference local="AD_CLIENT_ID" foreign="AD_CLIENT_ID"/>
      </foreign-key>
      <foreign-key foreignTable="AD_ORG" name="ADORG_KENTRY">
        <reference local="AD_ORG_ID" foreign="AD_ORG_ID"/>
      </foreign-key>
      <foreign-key foreignTable="K_SOURCE" name="KSOURCE_KENTRY">
        <reference local="K_SOURCE_ID" foreign="K_SOURCE_ID"/>
      </foreign-key>
      <foreign-key foreignTable="K_TOPIC" name="KTOPIC_KENTRY">
        <reference local="K_TOPIC_ID" foreign="K_TOPIC_ID"/>
      </foreign-key>
      <check name="K_ENTRY_ISACTIVE_CHECK"><![CDATA[ISACTIVE IN ('Y', 'N')]]></check>
      <check name="K_ENTRY_ISPUBLIC_CHECK"><![CDATA[ISPUBLIC IN ('Y', 'N')]]></check>
    </table>
  </database>
