<?xml version="1.0"?>
<database name="TABLE AD_PINSTANCE">
    <table name="AD_PINSTANCE" primaryKey="AD_PINSTANCE_KEY">
      <column name="AD_PINSTANCE_ID" primaryKey="true" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="AD_PROCESS_ID" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="RECORD_ID" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="ISPROCESSING" primaryKey="false" required="true" type="CHAR" size="1" default="N" autoIncrement="false"/>
      <column name="CREATED" primaryKey="false" required="true" type="TIMESTAMP" size="7" default="SYSDATE" autoIncrement="false"/>
      <column name="AD_USER_ID" primaryKey="false" required="false" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="UPDATED" primaryKey="false" required="false" type="TIMESTAMP" size="7" default="SYSDATE" autoIncrement="false"/>
      <column name="RESULT" primaryKey="false" required="false" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="ERRORMSG" primaryKey="false" required="false" type="NVARCHAR" size="2000" autoIncrement="false"/>
      <column name="AD_CLIENT_ID" primaryKey="false" required="false" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="AD_ORG_ID" primaryKey="false" required="false" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="CREATEDBY" primaryKey="false" required="false" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="UPDATEDBY" primaryKey="false" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <column name="ISACTIVE" primaryKey="false" required="false" type="CHAR" size="1" default="Y" autoIncrement="false"/>
      <check name="AD_PINSTANCE_ISACTIVE_CHECK" condition="IsProcessing in ('Y','N')"/> 
      <foreign-key foreignTable="AD_CLIENT" name="ADCLIENT_ADPINSTANCE">
        <reference local="AD_CLIENT_ID" foreign="AD_CLIENT_ID"/>
      </foreign-key>
      <foreign-key foreignTable="AD_ORG" name="ADORG_ADPINSTANCE">
        <reference local="AD_ORG_ID" foreign="AD_ORG_ID"/>
      </foreign-key>
      <foreign-key foreignTable="AD_PROCESS" name="ADPROCESS_ADPINSTANCE">
        <reference local="AD_PROCESS_ID" foreign="AD_PROCESS_ID"/>
      </foreign-key>
      <foreign-key foreignTable="AD_USER" name="ADUSER_PINSTANCE">
        <reference local="AD_USER_ID" foreign="AD_USER_ID"/>
      </foreign-key>
      <index name="AD_PINSTANCE_RECORD">
        <index-column name="AD_PROCESS_ID"/>
        <index-column name="RECORD_ID"/>
      </index>
    </table>
</database>