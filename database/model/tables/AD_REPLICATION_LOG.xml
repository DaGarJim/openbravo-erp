      </foreign-key>
      <check name="ADREPLOG_ISACTIVE_CHECK" condition="IsActive in (&apos;Y&apos;,&apos;N&apos;)"/>
      <check name="ADREPLOG_ISREPLICATED_CHECK" condition="IsReplicated in (&apos;Y&apos;,&apos;N&apos;)"/>
    </table>
  </database>
