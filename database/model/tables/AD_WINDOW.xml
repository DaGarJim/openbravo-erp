      </foreign-key>
      <foreign-key foreignTable="AD_ORG" name="WINDOWORG">
        <reference local="AD_ORG_ID" foreign="AD_ORG_ID"/>
      </foreign-key>
      <unique name="AD_WINDOW_NAME">
        <unique-column name="AD_CLIENT_ID"/>
        <unique-column name="NAME"/>
      </unique>
    </table>
</database>