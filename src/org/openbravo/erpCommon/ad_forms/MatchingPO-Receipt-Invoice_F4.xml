<REPORT>
	<template file="MatchingPO-Receipt-Invoice_F4.html"/>
	<PARAMETER id="paramLanguage" name="language" default=""/>
	<PARAMETER id="paramDirection" name="direction" default=""/>
	<PARAMETER id="paramProduct" name="product" attribute="value"/>
	<PARAMETER id="paramSearchMode" name="searchMode" attribute="value"/>
	<PARAMETER id="paramSourceDoc" name="sourceDoc" attribute="value"/>
	<PARAMETER id="paramFinalDoc" name="finalDoc" attribute="value"/>
	<PARAMETER id="paramMatchLine" name="matchLine" attribute="value"/>
	<structure name="structure1">
		<FIELD id="fieldClave" attribute="value">id</FIELD>
		<FIELD id="fieldTotalImporte" attribute="name" replace="xx">id</FIELD>
		<FIELD id="fieldTotalImporte" attribute="value">qty</FIELD>
		<FIELD id="fieldMatchedImporte" attribute="name" replace="xx">id</FIELD>
		<FIELD id="fieldMatchedImporte" attribute="value">qtyMatched</FIELD>
		<FIELD id="fieldProductLine" attribute="name" replace="xx">id</FIELD>
		<FIELD id="fieldProductLine" attribute="value">mProductId</FIELD>
		<FIELD id="fieldDocumentNo">documentno</FIELD>
		<FIELD id="fieldDate">curdate</FIELD>
		<FIELD id="fieldBpartnerDes">bpartnerName</FIELD>
		<FIELD id="fieldLineNo">line</FIELD>
		<FIELD id="fieldProductDes">productName</FIELD>
		<FIELD id="fieldQty" format="euroEdition">qty</FIELD>
		<FIELD id="fieldMatched" format="euroEdition">qtyMatched</FIELD>
		<SECTION id="sectionDetail"/>
	</structure>
	<DISCARD id="discard"/>
</REPORT>
