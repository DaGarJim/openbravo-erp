  <SqlMethod name="selectParent" type="preparedStatement" connection="true" return="string">
    <SqlMethodComment></SqlMethodComment>
    <Sql>
        SELECT C_ELEMENTVALUE_ID AS ID
        FROM C_ELEMENTVALUE
        WHERE VALUE = ?
