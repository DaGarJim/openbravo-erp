        VALUES (?,?,?,?,'Y',now(),'0',now(),'0',?,?,?,?,?,?,?,NULL,NULL,'Y','Y','Y','Y', 'N',NULL, 'N',NULL,COALESCE(SUBSTR(?,1,1),'S'))
