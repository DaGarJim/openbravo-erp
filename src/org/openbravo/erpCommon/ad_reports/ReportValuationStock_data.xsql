<SqlClass name="ReportValuationStockData" package="org.openbravo.erpCommon.ad_reports">
  <SqlClassComment></SqlClassComment>
  <SqlMethod name="select" type="preparedStatement" return="multiple">
    <SqlMethodComment></SqlMethodComment>
    <Sql>
    <![CDATA[
      SELECT M_PRODUCT_CATEGORY.NAME AS CATEGORY_NAME, A.M_PRODUCT_ID, AD_COLUMN_IDENTIFIER(TO_CHAR('M_Product'), TO_CHAR(M_PRODUCT.M_PRODUCT_ID), TO_CHAR('es_ES')) AS PRODUCT_NAME, 
      A.QTY, C_UOM.NAME AS UOM_NAME, GET_PRODUCT_COST(A.M_PRODUCT_ID,TO_DATE(?), NULL) AS AVERAGE_COST, 
      A.QTY *GET_PRODUCT_COST(A.M_PRODUCT_ID,TO_DATE(?), NULL) AS TOTAL_COST
      FROM M_PRODUCT_CATEGORY, M_PRODUCT,
      (SELECT M_PRODUCT_ID, SUM(MOVEMENTQTY) AS QTY, C_UOM_ID FROM M_TRANSACTION, M_LOCATOR
      WHERE M_TRANSACTION.M_LOCATOR_ID = M_LOCATOR.M_LOCATOR_ID
      AND MOVEMENTDATE < to_date(?)
      AND M_LOCATOR.M_WAREHOUSE_ID = TO_NUMBER(?)
      GROUP BY M_PRODUCT_ID, C_UOM_ID) A, C_UOM
      WHERE A.M_PRODUCT_ID = M_PRODUCT.M_PRODUCT_ID
      AND A.C_UOM_ID = C_UOM.C_UOM_ID
      AND M_PRODUCT.M_PRODUCT_CATEGORY_ID = M_PRODUCT_CATEGORY.M_PRODUCT_CATEGORY_ID
      AND 1=1
      AND A.QTY <>0
      ORDER BY CATEGORY_NAME, PRODUCT_NAME
     ]]></Sql>
      <Field name="rownum" value="count"/>
      <Parameter name="date"/>
      <Parameter name="date"/>
      <Parameter name="datePlus"/>
      <Parameter name="warehouse"/>
      <Parameter name="categoryProduct" optional="true" after="AND 1=1"><![CDATA[ AND M_PRODUCT.M_PRODUCT_CATEGORY_ID = TO_NUMBER(?) ]]></Parameter>
  </SqlMethod>
  <SqlMethod name="set" type="constant" return="multiple">
      <SqlMethodComment></SqlMethodComment>
      <Sql></Sql>
  </SqlMethod>
</SqlClass>
