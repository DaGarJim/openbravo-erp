	<queryString><![CDATA[SELECT COALESCE(SUM(AMTACCTDR),0) AS TOTALDR, COALESCE(SUM(AMTACCTCR),0) AS TOTALCR,
  COALESCE(SUM(AMTACCTDR-AMTACCTCR),0) AS TOTAL
FROM FACT_ACCT
where fact_acct.account_id = $P{AcctID}
  and ad_client_id in ($P!{USER_CLIENT})
  and ad_org_id in ($P!{USER_ORG})
  and dateacct >= $P{InitialYearDate}
  and dateacct <  $P{Date}
  $P!{auxPartner}]]></queryString>

