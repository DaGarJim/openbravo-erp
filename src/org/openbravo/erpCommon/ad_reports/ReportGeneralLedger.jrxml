	<queryString><![CDATA[SELECT FACT_ACCT.ACCTVALUE AS VALUE, C_BPARTNER.NAME AS PARTNER, DATEACCT, FACT_ACCT.ACCTDESCRIPTION AS NAME,
        AMTACCTDR, AMTACCTCR, FACT_ACCT_GROUP_ID, FACT_ACCT.ACCOUNT_ID AS ID, FACT_ACCT.FACT_ACCT_ID,
        FACT_ACCT.RECORD_ID, FACT_ACCT.AD_TABLE_ID, FACT_ACCT.DESCRIPTION, FACT_ACCT.C_BPARTNER_ID, 0 AS TOTALDR,
        0 AS TOTALCR, (AMTACCTDR-AMTACCTCR) AS TOTAL, 0 AS SALDO, '' AS PADRE, 0 AS TOTALACCTDR, 0 AS TOTALACCTCR, 0 AS TOTALACCTSUB
      FROM FACT_ACCT LEFT JOIN C_BPARTNER ON FACT_ACCT.C_BPARTNER_ID = C_BPARTNER.C_BPARTNER_ID
      WHERE FACT_ACCT.ACCOUNT_ID IN (
          SELECT c_elementvalue_id as name
          FROM C_ELEMENTVALUE
          WHERE c_elementvalue.ELEMENTLEVEL = 'S'
        )
        AND FACT_ACCT.AD_ORG_ID IN ($P!{Org})
        AND FACT_ACCT.AD_CLIENT_ID IN ($P!{USER_CLIENT})
        AND DATEACCT >= $P{dateFrom}
        AND DATEACCT < $P{dateTo}
      ORDER BY VALUE, DATEACCT, FACT_ACCT_GROUP_ID, FACT_ACCT_ID]]></queryString>

