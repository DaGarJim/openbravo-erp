    <FIELD id="fieldValue2">value</FIELD>
    <FIELD id="fieldPartner2">partner</FIELD>
    <FIELD id="fieldDateacct2">dateacct</FIELD>
    <FIELD id="fieldName2">name</FIELD>
    <FIELD id="fieldDescription2">description</FIELD>
    <FIELD id="fieldAmtacctdr2" format="euroInform">amtacctdr</FIELD>
    <FIELD id="fieldAmtacctcr2" format="euroInform">amtacctcr</FIELD>
    <FIELD id="fieldTotal2">total</FIELD>
    <FIELD id="fieldSaldo2" attribute="value">saldo</FIELD>
    <FIELD id="fieldId2" attribute="onclick" replace="xx">factAcctGroupId</FIELD>
    <FUNCTION id="functionSumLine21" name="SUM" attribute="value">total</FUNCTION>
    <FUNCTION id="functionSumLine22" name="ADD" arg1="functionSumLine21" arg2="fieldSaldo2" format="euroInform"/>

    <FUNCTION id="fieldTotalDebit2" name="SUM" format="euroInform">amtacctdr</FUNCTION>
    <FUNCTION id="fieldTotalCredit2" name="SUM" format="euroInform">amtacctcr</FUNCTION>
    <FUNCTION id="fieldTotalBalance2" name="SUM" format="euroInform">total</FUNCTION>

    <SUBREPORT id="reportTotals2" name="reportTotals2" report="org/openbravo/erpCommon/ad_reports/Totals"></SUBREPORT>
    <SUBREPORT id="reportAll2" name="reportAll2" report="org/openbravo/erpCommon/ad_reports/Totals"></SUBREPORT>

    <!--PARAMETER id="functionSumLine22" name="ADD" default="prueba"/-->
    <SECTION id="sectionPartner" field="partner"/>
    <SECTION id="sectionAmount2" field="value"/>
    <SECTION id="sectionDetail2"/>
  </structure>
</REPORT>
