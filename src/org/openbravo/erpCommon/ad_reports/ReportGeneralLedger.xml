    <SUBREPORT id="reportC_ACCTSCHEMA_ID" name="reportC_ACCTSCHEMA_ID" report="org/openbravo/erpCommon/reference/List">
      <ARGUMENT name="parameterListSelected" withId="paramC_ACCTSCHEMA_ID"/>
