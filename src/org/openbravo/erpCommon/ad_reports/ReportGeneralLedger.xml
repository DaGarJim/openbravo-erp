    <SUBREPORT id="reportAll" name="reportAll" report="org/openbravo/erpCommon/ad_reports/Totals"></SUBREPORT>

    <SUBREPORT id="reportC_ELEMENTVALUEFROM" name="reportC_ELEMENTVALUEFROM" report="org/openbravo/erpCommon/reference/List">
