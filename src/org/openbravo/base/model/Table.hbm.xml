<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.openbravo.base.model.Table" lazy="false" table="ad_table">
		<cache usage="read-write"/>
		
		<id name="id" type="string" column="ad_table_id">
			<generator class="uuid" />
		</id>
		
		<property name="active" type="yes_no" not-null="true" column="isactive"/>				
		<property name="name" not-null="true"/>

		<property name="className"/>
		
		<property name="tableName"/>
		<property name="view" type="yes_no" column="isview"/>
		<property name="deletable" type="yes_no" column="isdeleteable"/>
		<property name="accessLevel" column="accesslevel"/>
		
		<bag name="columns" cascade="all" lazy="false" order-by="position asc">
			<key column="ad_table_id"/>
            <one-to-many class="org.openbravo.base.model.Column"/>
        </bag>
                		
		<many-to-one name="thePackage" not-null="true" class="org.openbravo.base.model.Package" column="ad_package_id"/>
		       
	</class>
</hibernate-mapping>