<REPORT>
  <template file="list13mod.html" />

  <structure name="structure1">
    <SQL>
      SELECT Employee, Year, Month, Day, Hours FROM Hours
    </SQL>

    <FIELD id="fieldEmployee">Employee</FIELD>
    <FIELD id="fieldYear">Year</FIELD>
    <FIELD id="fieldMonth">Month</FIELD>
    <FIELD id="fieldDay">Day</FIELD>
    <FIELD id="fieldHours">Hours</FIELD>

    <FUNCTION id="funcCountParts" name="COUNT">Hours</FUNCTION>
    <FUNCTION id="funcSumHours" name="SUM">Hours</FUNCTION>

    <PARAMETER id="param3" name="Param3" default="3"/>
    <FUNCTION id="funcModule" name="MODULE" arg1="fieldHours" arg2="param3"/>

    <PARAMETER id="param5" name="Param5" default="5"/>
    <FUNCTION id="funcGt" name="GT" arg1="fieldHours" arg2="param5"/>
    <FUNCTION id="funcLt" name="LT" arg1="fieldHours" arg2="param5"/>
    <FUNCTION id="fieldHours" name="EQUAL" attribute="class" replace="xx" arg1="funcGt" format="integerInform"/>

    <SECTION id="sectionDetail"/>
  </structure>

</REPORT>
