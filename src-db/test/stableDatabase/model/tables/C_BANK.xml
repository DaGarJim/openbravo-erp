      <column name="SWIFTCODE" primaryKey="false" required="false" type="NVARCHAR" size="20" autoIncrement="false"/>
      <column name="ISOWNBANK" primaryKey="false" required="false" type="CHAR" size="1" default="N" autoIncrement="false"/>
