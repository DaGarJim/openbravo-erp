      <check name="CBANK_ISACTIVE_CHECK"><![CDATA[ISACTIVE IN ('Y', 'N')]]></check>
      <check name="CBANK_ISOWNBANK_CHECK"><![CDATA[ISOWNBANK IN ('Y', 'N')]]></check>
    </table>
  </database>
