      <column name="INE_NUMBER" primaryKey="false" required="false" type="NVARCHAR" size="9" autoIncrement="false"/>
      <column name="C_BPARTNER_ID" primaryKey="false" required="false" type="DECIMAL" size="10,0" autoIncrement="false"/>
      <foreign-key foreignTable="AD_CLIENT" name="ADCLIENT_CBANK">
