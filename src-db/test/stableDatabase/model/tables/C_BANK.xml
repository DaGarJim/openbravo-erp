<?xml version="1.0"?>
  <database name="TABLE C_BANK">
    <table name="C_BANK" primaryKey="C_BANK_KEY">
      <column name="C_BANK_ID" primaryKey="true" required="true" type="DECIMAL" size="10,0" autoIncrement="false"/>
