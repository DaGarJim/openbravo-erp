  <PA_MEASURECALC PA_MEASURECALC_ID="100" AD_CLIENT_ID="0" AD_ORG_ID="0" ISACTIVE="Y" CREATED="2001-04-26 18:38:23.0" CREATEDBY="100" UPDATED="2001-04-26 18:43:35.0" UPDATEDBY="0" NAME="Invoiced Net Revenue" DESCRIPTION="Invoiced net revenue, without tax and charges" SELECTCLAUSE="SELECT SUM(il.LineNetAmt) FROM C_Invoice ih, C_InvoiceLine il" WHERECLAUSE="WHERE ih.C_Invoice_ID=il.C_Invoice_ID AND ih.IsSOTrx='Y'" DATECOLUMN="ih.DateInvoiced" ORGCOLUMN="ih.AD_Org_ID" BPARTNERCOLUMN="ih.C_BPartner_ID" PRODUCTCOLUMN="il.M_Product_ID" />
