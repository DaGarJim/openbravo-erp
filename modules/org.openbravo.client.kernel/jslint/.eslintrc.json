{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "globals": {
    "OB": true,
    "Backbone": false,
    "_": false,
    "isc": false,
    "BigDecimal": false
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "semi": ["error", "always"],
    "eol-last": ["error", "always"],

    "no-unused-vars": ["warn", {
      "args": "none"
    }],
    "no-console": ["warn"],
    "no-useless-escape": ["warn"]
  }
}
