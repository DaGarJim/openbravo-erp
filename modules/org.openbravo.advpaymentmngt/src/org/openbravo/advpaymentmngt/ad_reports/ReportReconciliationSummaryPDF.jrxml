	<parameter name="SubRptOutUnrecAmt" isForPrompting="false" class="net.sf.jasperreports.engine.JasperReport"/>
	<queryString><![CDATA[SELECT fin_reconciliation.fin_reconciliation_id, 
fin_reconciliation.dateto as EndDate, 
fin_reconciliation.statementdate, fin_reconciliation.endingbalance, 
fin_reconciliation.startingbalance, 
unrec.unrec_amt, outstanding.payment_amt, outstanding.deposit_amt
FROM fin_reconciliation
LEFT JOIN 
	(SELECT fin_bankstatement.fin_reconciliation_id, 
	  COALESCE(sum(fin_bankstatementline.cramount - fin_bankstatementline.dramount), 0) AS unrec_amt
	FROM fin_bankstatementline, fin_bankstatement
	WHERE fin_bankstatementline.fin_finacc_transaction_id IS NULL 
	   AND fin_bankstatementline.fin_bankstatement_id = fin_bankstatement.fin_bankstatement_id
	GROUP BY fin_bankstatement.fin_reconciliation_id) 
	unrec ON fin_reconciliation.fin_reconciliation_id = unrec.fin_reconciliation_id
LEFT JOIN 
	(SELECT fin_reconciliation.fin_reconciliation_id, 
	   COALESCE(sum(fin_finacc_transaction.paymentamt), 0) AS payment_amt, 
	   COALESCE(sum(fin_finacc_transaction.depositamt), 0) AS deposit_amt
	FROM fin_finacc_transaction, fin_reconciliation
	WHERE fin_finacc_transaction.fin_reconciliation_id IS NULL 
  	AND fin_finacc_transaction.statementdate <= fin_reconciliation.dateto
	GROUP BY fin_reconciliation.fin_reconciliation_id) 
	outstanding ON fin_reconciliation.fin_reconciliation_id = outstanding.fin_reconciliation_id
WHERE fin_reconciliation.fin_reconciliation_id='to-do1']]></queryString>

